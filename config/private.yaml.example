# Generated from this template by scripts/gen-configs.sh
# To change values, edit .env and run: make gen-configs

jwt_key: "${JWT_KEY}"
encryption_key: "${ENCRYPTION_KEY}"

pg:
  host: "postgres"
  port: 5432
  user: "${POSTGRES_USER}"
  password: "${POSTGRES_PASSWORD}"
  dbname: "${POSTGRES_DB}"

email:
  smtp_server: "${SMTP_SERVER}"
  smtp_port: ${SMTP_PORT}
  username: "${SMTP_USERNAME}"
  password: "${SMTP_PASSWORD}"
  sender_name: "${SMTP_SENDER_NAME}"
  timeout: 10
